<?xml version="1.0" encoding="utf-8"?>

<resources>

  <eat-comment/>

  <declare-styleable name="MessagesIndicator">
    <attr format="string" name="title"/>
    <attr format="boolean" name="values_visible"/>
    <attr format="string" name="idle_text"/>
    <attr format="enum" name="visible_range">
      <enum name="one_minute" value="0"/>
      <enum name="ten_minutes" value="1"/>
      <enum name="one_hour" value="2"/>
      <enum name="eight_hours" value="3"/>
      <enum name="one_day" value="4"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="QueuedMessagesIndicator">
    <attr format="color" name="ready_color"/>
    <attr format="color" name="unacked_color"/>
    <attr format="color" name="total_color"/>
  </declare-styleable>

  <declare-styleable name="MessageRatesIndicator">
    <attr format="color" name="publish_color"/>
    <attr format="color" name="confirm_color"/>
    <attr format="color" name="publish_in_color"/>
    <attr format="color" name="publish_out_color"/>
    <attr format="color" name="deliver_color"/>
    <attr format="color" name="redelivered_color"/>
  </declare-styleable>

  <declare-styleable name="PortNumberPicker">
    <attr format="integer" name="initial_port_number"/>
  </declare-styleable>

</resources>
