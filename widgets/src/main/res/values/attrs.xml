<?xml version="1.0" encoding="utf-8"?>

<resources>

  <eat-comment/>

  <declare-styleable name="QueuedMessagesIndicator">
    <attr format="string" name="title"/>
    <attr format="color" name="readyLineColor"/>
    <attr format="color" name="readyButtonColor"/>
    <attr format="color" name="unackedLineColor"/>
    <attr format="color" name="unackedButtonColor"/>
    <attr format="color" name="totalLineColor"/>
    <attr format="color" name="totalButtonColor"/>
    <attr format="boolean" name="valuesVisible"/>
    <attr format="string" name="idleText"/>
    <attr format="enum" name="visibleRange">
      <enum name="one_minute" value="0"/>
      <enum name="ten_minutes" value="1"/>
      <enum name="one_hour" value="2"/>
      <enum name="eight_hours" value="3"/>
      <enum name="one_day" value="4"/>
    </attr>
  </declare-styleable>

</resources>
